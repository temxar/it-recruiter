    "lint:css": "stylelint --fix src/**/*.{css,scss}",
    "format": "prettier --write {src,webpack}/**/*.{ts,tsx,js}",
    "f": "hygen feature new",
    "ff": "hygen feature folder",
    "s": "hygen shared new",
    "ss": "hygen shared folder",
    "t": "hygen template new",
    "p": "hygen page new",
    "l": "hygen layout new"
  },
