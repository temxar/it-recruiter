  "scripts": {
    "dev": "kill-port 9006 && cross-env NODE_ENV=development cross-env BUILD=development webpack serve --config webpack/webpack.config.js",
    "build:dev": "rimraf build/ && cross-env NODE_ENV=development cross-env BUILD=production webpack --config webpack/webpack.config.js",
    "build:prod": "rimraf build/ && cross-env NODE_ENV=production cross-env BUILD=production webpack --config webpack/webpack.config.js",
